{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "MODEL_SIZE": "small",
    "DEBUG": "false",
    "LOG_LEVEL": "info"
  },
  "functions": {
    "app/main.py": {
      "maxDuration": 60
    }
  }
}